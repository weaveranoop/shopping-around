<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>shopping-around</display-name>
  <welcome-file-list>
    <welcome-file>SignupPage.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>SpringMvc</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>SpringMvc</servlet-name>
    <url-pattern>*.htm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SignUp</servlet-name>
    <servlet-class>com.snap.web.SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignUp</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.snap.web.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.snap.web.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Scat</servlet-name>
    <servlet-class>com.snap.web.SearchByCategory</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Scat</servlet-name>
    <url-pattern>/scat</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Sarea</servlet-name>
    <servlet-class>com.snap.web.SearchByAreaCategory</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Sarea</servlet-name>
    <url-pattern>/sarea</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Prod</servlet-name>
    <servlet-class>com.snap.web.SearchProduct</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Prod</servlet-name>
    <url-pattern>/prod</url-pattern>
  </servlet-mapping>
  <error-page>
    <location>/Error.jsp</location>
</error-page>
</web-app>